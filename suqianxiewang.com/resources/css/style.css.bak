/*****************全局设置+导航****************************/
*{margin:0;padding:0;}
html{height:100%;max-height:100%;border:0;background:#fff;}
body{height:100%;font-family: Arial, sans-serif; width:100%;}
#nav_ul{ list-style-type:none;}
a:link,a:visited{outline:none;text-decoration:none;}
h1, h2, h3, h4, h5, h6 {
                font-family: Helvetica, Arial, sans-serif;
                color: #222;
                font-weight: bold;
                }
h1            { font-family: Calibri, Helvetica, Arial, sans-serif; font-size: 31px; font-weight: bold; color: #fff; 
				  position: absolute; top: -1000px;  /* Remove this line and delete the logo (in the HTML) if you don't want an image as a logo */
				}
h2            { font-size: 26px; padding: 0 0 10px 0; }
h3            { font-size: 17px; padding: 0 0 10px 0; }
h4            { font-size: 16px; padding: 0 0 5px 0; }
h5            { font-size: 14px; padding: 0 0 5px 0; }
h6            { font-size: 12px; padding: 0 0 5px 0; }
a           { color: #1D97D9; text-decoration: none; }
a:hover     { color: #2B73EE; }
.case {color:#000;}
.headli {color:#D0D0D3;}
.headli:hover{color:#fff;}
.case a:hover{color:#2B73EE;}
.case a:active     { color: #2B73EE; }
.case a:focus     { outline: 1px; }

a:active     { color: #2B73EE; }
a:focus     { outline: 1px; }

strong       { font-weight: bold; color: #333; }

small   		{ font-size: 0.85em; }

pre           { font-family: monospace; }

p {
                padding: 5px 0 10px 0;
                line-height: 1.6em;
                }
#wrap{width:100%;top:0;}
#topBar{color:#fff;height:25px;background:#131623;overflow:hidden;font:12px/28px Arial;text-align:right;}
#topBar a{color:#FFF;background:#7A7A7A;padding:0px 5px;border-radius:10px;}
#nav{height:50px; width:100%;overflow:hidden;border-top:1px solid #999;border-bottom::1px solid #666;background:#000 url(../img/ibm-bg.png) repeat-x;}
#nav .logo{float:left;margin:0 20px;}
#nav .logo a{float:left;width:343px;height:50px;overflow:hidden;text-indent:-9999px;background:url(../img/ksdlogo.png) no-repeat;}
#nav ul,#nav ul li{float:left;}
#nav ul li a{float:left;color:#CCC;height:50px;font:16px/50px Arial;padding:0 20px;}
#nav ul li a:hover{color:#FFF;background:url(../img/ibm-bg.png) 0 -50px repeat-x;}
.videoList-container2{ margin-bottom:24px;}
.navitem-list{ float:left; padding:10px 20px;margin:0 auto; height:200px;}
/***********************滚动新闻****************************************/
#news-feed{border-top: 1px solid rgba(0, 0, 0, .1);border-top: 1px solid #CCC	9; border-bottom:1px solid #ccc; position: relative;height: 29px; background-color:#e5e5e5; background-image:url(../img/bg-news-box.gif); padding:0 auto; text-align:center;}
.scroll{height:25px;line-height:25px;overflow:hidden; text-align:left; font-size:13px;}
.scroll a{height:25px;display:block; padding-top:4px;color:#000;}
/***********************导航pane*******************************/
#itemMun
{
	border-top:1px solid #4978B1;
	height:100px;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0, #02416B), color-stop(1, #5582BC));
	background: -moz-linear-gradient(top, #02416B, #5582BC);
	background: -o-linear-gradient(top, #02416B, #5582BC);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#02416B', endColorstr='#5582BC',GradientType='0');
	}
#box{height:20px;width:100%;position:relative; left:0px;white-space:nowrap; }
#box span{display:inline-block;width:100%;}
#box span ul li{background:url(../img/bullets.gif) no-repeat -2px -198px; padding-left:10px; margin-top:10px;font-family: Arial, sans-serif; color:#D0D0D3; list-style:none;}
#box span .droplist{ padding:0 auto; margin-top:20px; text-align:left;}
#box span div h3{
color: rgba(255, 255, 255, .75);
color: #00B0DA 9;
width: 140px;
padding-left: 0;
font-weight: bold;
height:15px;

}
h3 {
display: block;
font-size: 1.17em;
-webkit-margin-before: 1em;
-webkit-margin-after: 1em;
-webkit-margin-start: 0px;
-webkit-margin-end: 0px;
font-weight: bold;
}
#box span div .ti{ color:#FFE14F;}
/***************Banner *****************/
#slide-index{min-width:1000px;width:auto; height:396px; overflow:hidden; position:relative;}
#slide-index .slides{ min-width:1000px; width:auto; height:396px; }
#slide-index .slide{ min-width:1000px; height:396px; float:left; position:relative; }
#slide-index .image{ z-index:1;position:absolute; }
#slide-index .text, #slide-index .button { z-index:2;position:absolute; top:-500px; }
#slide-index .button{ display:none; width:138px;}
#slide-index .control{ position:absolute; bottom:0px; width:100%; margin-left:70%; height:30px; cursor:pointer; z-index:40;}
#slide-index .control a{ width:60px; height:30px; cursor:pointer; display:inline-block; background-repeat:no-repeat; margin-right:6px; opacity:0.6; filter:alpha(opacity=60);}
#slide-index .control a:hover, #slide-index .control a.active{  opacity:1; filter:alpha(opacity=100);}
.autoMaxWidth{ overflow:hidden; position:relative;height:232px; }

/******************body******************************/
#body_container{width: 100%; margin-top:1%; text-align:center; padding:0 auto;}
.body_container_div_con{display: inline;  float: left; border: 2px solid rgb(240,240,240); margin-right:1%;width:318px; height:98px;  text-align:left;}
.bb1{background:#f0f0f0 url(../img/bb1.jpg) no-repeat;}
.bb2{background:#f0f0f0 url(../img/bb2.jpg) no-repeat;}
.bb3{background:#f0f0f0 url(../img/bb3.jpg) no-repeat;}
.bb_title { padding-left:145px; padding-top:5px;color:#00649D; font-size:18px;  font-weight:800;}
.bb_con{ padding-left:145px; padding-top:0px;font-size:12px;line-height:16px;color:#000;}
/************************foot**************************/

#seo-footer 
{
margin-top:30px;
padding:0 auto;
width:100%;
height:280px;
font-size:1em;
overflow:hidden;
text-align:center;
border-top: 1px solid #666;
background: #404040;
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#474A50', endColorstr='#0F161A');
background: -webkit-linear-gradient(top , #474A50, #0F161A);
background:  -moz-linear-gradient(top , #474A50, #0F161A) repeat 0 0;
background:  -o-linear-gradient(top , #474A50, #0F161A) repeat 0 0;
}
#seo-footer .bound{ width:960px; overflow:hidden; margin:0 auto; height:70%;}
#seo-footer .section{ float:left; min-width:100px; padding:15px 0px 0px 0px;}
#seo-footer .section ul li{background:url(../img/bullets.gif) no-repeat -2px -300px; padding-left:10px; margin-top:10px;font-family: Arial, sans-serif; color:#D0D0D3; list-style:none;}
#seo-footer .section ul li a{color:#999;font-family: Arial, sans-serif; font-size:12px; }
#seo-footer .section ul li a:hover{color:#FFF;}
#seo-footer .information {width:15%;margin-right:1%;text-align:left;}
#seo-footer .information ul{text-align:left;margin-left:0px;}
#seo-footer .information .infotitle{border-left:0px solid #FCB712; padding-left:0px;color:#FCB712; font-size:1em;}
#footer{ background:url(../img/hw_000450.jpg) repeat-x; padding-top:25px; min-width:1000px; width:auto !important; margin:0px auto; background-color:#F1F1F1;}
#footbottom{text-align:center;background:#333;color:#999; width:100%;  margin-bottom:0px; font-family: Arial, sans-serif; font-size:13px; line-height:20px;height:30%; padding:5px 0px;}

